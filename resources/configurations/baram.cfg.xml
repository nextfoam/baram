<?xml version="1.0" encoding="UTF-8"?>
<configuration version="7" xmlns="http://www.baramcfd.org/baram">
    <general>
        <flowType>incompressible</flowType>
        <timeTransient>false</timeTransient>
        <solverType>pressureBased</solverType>
        <operatingConditions>
            <gravity disabled="false">
                <direction>
                    <x>0</x>
                    <y>0</y>
                    <z>0</z>
                </direction>
            </gravity>
            <pressure>101325</pressure>
        </operatingConditions>
        <atmosphericBoundaryLayer>
            <flowDirection>
                <x>1</x>
                <y>0</y>
                <z>0</z>
            </flowDirection>
            <groundNormalDirection>
                <x>0</x>
                <y>0</y>
                <z>1</z>
            </groundNormalDirection>
            <referenceFlowSpeed>10</referenceFlowSpeed>
            <referenceHeight>10</referenceHeight>
            <surfaceRoughnessLength>0.01</surfaceRoughnessLength>
            <minimumZCoordinate>0.0</minimumZCoordinate>
            <userDefinedScalars/>
        </atmosphericBoundaryLayer>
    </general>

    <referenceValues>
        <area>1</area>
        <density>1.225</density>
        <length>1</length>
        <velocity>1</velocity>
        <pressure>0</pressure>
        <referencePressureLocation>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </referencePressureLocation>
    </referenceValues>

    <materials/>

    <models>
        <multiphaseModels>
            <model>off</model>
        </multiphaseModels>
        <energyModels>off</energyModels>
        <turbulenceModels>
            <model>k-epsilon</model>
            <k-epsilon>
                <model>standard</model>
                <realizable>
                    <nearWallTreatment>standardWallFunctions</nearWallTreatment>
                    <threshold>60</threshold>
                    <blendingWidth>10</blendingWidth>
                </realizable>
            </k-epsilon>
            <k-omega>
                <model>SST</model>
            </k-omega>
            <des>
                <RANSModel>spalartAllmaras</RANSModel>
                <spalartAllmarasOptions>
                    <lowReDamping>true</lowReDamping>
                </spalartAllmarasOptions>
                <DESOptions>
                    <delayedDES>false</delayedDES>
                </DESOptions>
                <modelConstants>
                    <DES>0.65</DES>
                    <DESKOmega>0.82</DESKOmega>
                    <DESKEpsilon>0.6</DESKEpsilon>
                </modelConstants>
                <shieldingFunctions>DDES</shieldingFunctions>
                <lengthScaleModel>cubeRootVol</lengthScaleModel>
            </des>
            <les>
                <subgridScaleModel>Smagorinsky</subgridScaleModel>
                <lengthScaleModel>cubeRootVol</lengthScaleModel>
                <modelConstants>
                    <k>0.094</k>
                    <e>1.048</e>
                    <w>0.325</w>
                </modelConstants>
            </les>
            <energyPrandtlNumber>0.85</energyPrandtlNumber>
            <wallPrandtlNumber>0.85</wallPrandtlNumber>
            <turbulentSchmidtNumber>0.7</turbulentSchmidtNumber>
        </turbulenceModels>
        <speciesModels>off</speciesModels>
        <userDefinedScalars>
            <scalar scalarID="0">
                <fieldName/>
                <region/>
                <material>0</material>
                <diffusivity>
                    <specificationMethod>constant</specificationMethod>
                    <constant>0</constant>
                    <laminarAndTurbulentViscosity>
                        <laminarViscosityCoefficient>1</laminarViscosityCoefficient>
                        <turbulentViscosityCoefficient>1</turbulentViscosityCoefficient>
                    </laminarAndTurbulentViscosity>
                </diffusivity>
            </scalar>
        </userDefinedScalars>
    </models>

    <regions/>

    <numericalConditions>
        <pressureVelocityCouplingScheme>SIMPLE</pressureVelocityCouplingScheme>
        <densityBasedSolverParameters>
            <formulation>implicit</formulation>
            <fluxType>roeFlux</fluxType>
            <entropyFixCoefficient>0.5</entropyFixCoefficient>
            <cutOffMachNumber>0.729</cutOffMachNumber>
        </densityBasedSolverParameters>
        <useMomentumPredictor>true</useMomentumPredictor>
        <discretizationSchemes>
            <time>firstOrderImplicit</time>
            <momentum>secondOrderUpwind</momentum>
            <energy>secondOrderUpwind</energy>
            <turbulentKineticEnergy>firstOrderUpwind</turbulentKineticEnergy>
            <volumeFraction>firstOrderUpwind</volumeFraction>
            <pressure>linear</pressure>
            <scalar>secondOrderUpwind</scalar>
            <species>firstOrderUpwind</species>
        </discretizationSchemes>
        <underRelaxationFactors>
            <pressure>0.3</pressure>
            <pressureFinal>1</pressureFinal>
            <density>0.9</density>
            <densityFinal>1</densityFinal>
            <momentum>0.7</momentum>
            <momentumFinal>1</momentumFinal>
            <energy>1.0</energy>
            <energyFinal>1</energyFinal>
            <turbulence>0.7</turbulence>
            <turbulenceFinal>1</turbulenceFinal>
            <volumeFraction>0.7</volumeFraction>
            <volumeFractionFinal>1</volumeFractionFinal>
            <scalar>0.7</scalar>
            <scalarFinal>1</scalarFinal>
            <species>1.0</species>
            <speciesFinal>1</speciesFinal>
        </underRelaxationFactors>
        <highOrderTermRelaxation disabled="true">
            <relaxationFactor>0.5</relaxationFactor>
        </highOrderTermRelaxation>
        <maxIterationsPerTimeStep>20</maxIterationsPerTimeStep>
        <numberOfCorrectors>4</numberOfCorrectors>
        <multiphase>
            <maxIterationsPerTimeStep>2</maxIterationsPerTimeStep>
            <numberOfCorrectors>1</numberOfCorrectors>
            <useSemiImplicitMules>true</useSemiImplicitMules>
            <phaseInterfaceCompressionFactor>1</phaseInterfaceCompressionFactor>
            <numberOfMulesIterations>3</numberOfMulesIterations>
        </multiphase>
        <convergenceCriteria>
            <pressure>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </pressure>
            <density>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </density>
            <momentum>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </momentum>
            <energy>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </energy>
            <turbulence>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </turbulence>
            <volumeFraction>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </volumeFraction>
            <scalar>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </scalar>
            <species>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </species>
        </convergenceCriteria>
        <advanced>
            <limits>
                <minimumStaticTemperature>1</minimumStaticTemperature>
                <maximumStaticTemperature>5000</maximumStaticTemperature>
                <maximumViscosityRatio>1e5</maximumViscosityRatio>
            </limits>
            <equations>
                <flow>true</flow>
                <energy disabled="false">
                    <includeViscousDissipationTerms>false</includeViscousDissipationTerms>
                    <includeKineticEnergyTerms>false</includeKineticEnergyTerms>
                    <includePressureWorkTerms>true</includePressureWorkTerms>
                </energy>
                <UDS>true</UDS>
                <species>true</species>
            </equations>
        </advanced>
    </numericalConditions>

    <monitors>
        <forces/>
        <points/>
        <surfaces/>
        <volumes/>
    </monitors>

    <runCalculation>
        <runConditions>
            <numberOfIterations>1000</numberOfIterations>
            <timeSteppingMethod>fixed</timeSteppingMethod>
            <timeStepSize>0.01</timeStepSize>
            <maxCourantNumber>1</maxCourantNumber>
            <VoFMaxCourantNumber>1</VoFMaxCourantNumber>
            <endTime>10</endTime>
            <reportIntervalSteps>100</reportIntervalSteps>
            <reportIntervalSeconds>0.1</reportIntervalSeconds>
            <retainOnlyTheMostRecentFiles>false</retainOnlyTheMostRecentFiles>
            <maximumNumberOfDataFiles>1</maximumNumberOfDataFiles>
            <dataWriteFormat>binary</dataWriteFormat>
            <dataWritePrecision>6</dataWritePrecision>
            <timePrecision>6</timePrecision>
        </runConditions>
        <parallel>
            <numberOfCores>1</numberOfCores>
            <localhost>true</localhost>
            <hostfile/>
        </parallel>
        <batch>
            <parameters/>
        </batch>
    </runCalculation>
</configuration>