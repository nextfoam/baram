<?xml version="1.0" encoding="UTF-8"?>
<configuration version="12" xmlns="http://www.baramcfd.org/baram">
    <general>
        <flowType>incompressible</flowType>
        <timeTransient>false</timeTransient>
        <solverType>pressureBased</solverType>
        <operatingConditions>
            <gravity disabled="false">
                <direction>
                    <x>0</x>
                    <y>0</y>
                    <z>0</z>
                </direction>
            </gravity>
            <pressure>101325</pressure>
        </operatingConditions>
        <atmosphericBoundaryLayer>
            <flowDirection>
                <specMethod>direct</specMethod>
                <value>
                    <x>1</x>
                    <y>0</y>
                    <z>0</z>
                </value>
            </flowDirection>
            <groundNormalDirection>
                <x>0</x>
                <y>0</y>
                <z>1</z>
            </groundNormalDirection>
            <referenceFlowSpeed>10</referenceFlowSpeed>
            <referenceHeight>10</referenceHeight>
            <surfaceRoughnessLength>0.01</surfaceRoughnessLength>
            <minimumZCoordinate>0.0</minimumZCoordinate>
            <pasquillStability disabled="true">
                <stabilityClass>D</stabilityClass>
                <latitude>37</latitude>
                <surfaceHeatFlux>0</surfaceHeatFlux>
                <referenceDensity>1.225</referenceDensity>
                <referenceSpecificHeat>1006.0</referenceSpecificHeat>
                <referenceTemperature>298.15</referenceTemperature>
            </pasquillStability>
        </atmosphericBoundaryLayer>
    </general>

    <referenceValues>
        <area>1</area>
        <density>1.225</density>
        <length>1</length>
        <velocity>1</velocity>
        <pressure>0</pressure>
        <referencePressureLocation>
            <x>0</x>
            <y>0</y>
            <z>0</z>
        </referencePressureLocation>
    </referenceValues>

    <materials>
        <material mid="1">
            <name>air</name>
            <type>nonmixture</type>
            <chemicalFormula>air</chemicalFormula>
            <phase>gas</phase>
            <molecularWeight>28.966</molecularWeight>
            <absorptionCoefficient>0</absorptionCoefficient>
            <density>
                <specification>constant</specification>
                <constant>1.225</constant>
                <polynomial>0</polynomial>
                <boussinesq>
                    <rho0>1</rho0>
                    <T0>300</T0>
                    <beta>3e-03</beta>
                </boussinesq>
            </density>
            <specificHeat>
                <specification>constant</specification>
                <constant>1006</constant>
                <polynomial>0</polynomial>
                <janaf>
                    <lowTemperature>200</lowTemperature>
                    <commonTemperature>1000</commonTemperature>
                    <highTemperature>6000</highTemperature>
                    <lowCoefficients>0 0 0 0 0 0 0</lowCoefficients>
                    <highCoefficients>0 0 0 0 0 0 0</highCoefficients>
                </janaf>
            </specificHeat>
            <transport>
                <specification>constant</specification>
                <viscosity>1.79E-05</viscosity>
                <thermalConductivity>0.0245</thermalConductivity>
                <polynomial>
                    <viscosity>0.1</viscosity>
                    <thermalConductivity>0</thermalConductivity>
                </polynomial>
                <sutherland>
                    <coefficient>1.46E-06</coefficient>
                    <temperature>110.4</temperature>
                </sutherland>
            </transport>
            <criticalTemperature>132.531</criticalTemperature>
            <criticalPressure>3.79E+06</criticalPressure>
            <criticalSpecificVolume>342.685</criticalSpecificVolume>
            <tripleTemperature>298.15</tripleTemperature>
            <triplePressure>101325</triplePressure>
            <normalBoilingTemperature>373.15</normalBoilingTemperature>
            <standardStateEnthalpy>0.0</standardStateEnthalpy>
            <referenceTemperature>298.15</referenceTemperature>
            <acentricFactor>0.0335</acentricFactor>
        </material>
    </materials>

    <models>
        <multiphaseModels>
            <model>off</model>
        </multiphaseModels>
        <energyModels>off</energyModels>
        <turbulenceModels>
            <model>k-epsilon</model>
            <k-epsilon>
                <model>standard</model>
                <realizable>
                    <nearWallTreatment>standardWallFunctions</nearWallTreatment>
                    <threshold>60</threshold>
                    <blendingWidth>10</blendingWidth>
                </realizable>
            </k-epsilon>
            <k-omega>
                <model>SST</model>
            </k-omega>
            <des>
                <RANSModel>spalartAllmaras</RANSModel>
                <spalartAllmarasOptions>
                    <lowReDamping>true</lowReDamping>
                </spalartAllmarasOptions>
                <DESOptions>
                    <delayedDES>false</delayedDES>
                </DESOptions>
                <modelConstants>
                    <DES>0.65</DES>
                    <DESKOmega>0.82</DESKOmega>
                    <DESKEpsilon>0.6</DESKEpsilon>
                </modelConstants>
                <shieldingFunctions>DDES</shieldingFunctions>
                <lengthScaleModel>cubeRootVol</lengthScaleModel>
            </des>
            <les>
                <subgridScaleModel>Smagorinsky</subgridScaleModel>
                <lengthScaleModel>cubeRootVol</lengthScaleModel>
                <modelConstants>
                    <k>0.094</k>
                    <e>1.048</e>
                    <w>0.325</w>
                </modelConstants>
            </les>
            <energyPrandtlNumber>0.85</energyPrandtlNumber>
            <wallPrandtlNumber>0.85</wallPrandtlNumber>
            <turbulentSchmidtNumber>0.7</turbulentSchmidtNumber>
        </turbulenceModels>
        <speciesModels>off</speciesModels>
        <userDefinedScalars>
            <scalar scalarID="0">
                <fieldName/>
                <region/>
                <material>0</material>
                <diffusivity>
                    <specificationMethod>constant</specificationMethod>
                    <constant>0</constant>
                    <laminarAndTurbulentViscosity>
                        <laminarViscosityCoefficient>1</laminarViscosityCoefficient>
                        <turbulentViscosityCoefficient>1</turbulentViscosityCoefficient>
                    </laminarAndTurbulentViscosity>
                </diffusivity>
            </scalar>
        </userDefinedScalars>
        <DPMModels>
            <properties>
                <particleType>none</particleType>
                <inert>
                    <inertParticle>0</inertParticle>
                </inert>
                <droplet>
                    <composition/>
                    <temperature>300</temperature>
                </droplet>
                <numericalConditions>
                    <interactionWithContinuousPhase>true</interactionWithContinuousPhase>
                    <maxParticleCourantNumber>0.3</maxParticleCourantNumber>
                    <DPMIterationInterval>10</DPMIterationInterval>
                    <nodeBasedAveraging>true</nodeBasedAveraging>
                    <trackingScheme>implicit</trackingScheme>
                </numericalConditions>
                <kinematicModel>
                    <dragForce>
                        <specification>sphereDrag</specification>
                        <nonSphereDrag>
                            <shapeFactor>0.9</shapeFactor>
                        </nonSphereDrag>
                        <TomiyamaDrag>
                            <surfaceTension>0.9</surfaceTension>
                            <contamination>pure</contamination>
                        </TomiyamaDrag>
                    </dragForce>
                    <liftForce>
                        <specification>none</specification>
                        <TomiyamaLift>
                            <surfaceTension>0.9</surfaceTension>
                        </TomiyamaLift>
                    </liftForce>
                    <gravity>false</gravity>
                    <pressureGradient>false</pressureGradient>
                    <brownianMotionForce disabled="true">
                        <molecularFreePathLength>0.9</molecularFreePathLength>
                        <useTurbulence>false</useTurbulence>
                    </brownianMotionForce>
                </kinematicModel>
                <turbulentDispersion>none</turbulentDispersion>
                <heatTransfer>
                    <specification>none</specification>
                    <ranzMarshall>
                        <birdCorrection>true</birdCorrection>
                    </ranzMarshall>
                </heatTransfer>
                <evaporation>
                    <model>none</model>
                    <enthalpyTransferType>enthalpyDifference</enthalpyTransferType>
                </evaporation>
            </properties>
            <injections/>
        </DPMModels>
    </models>

    <regions/>

    <numericalConditions>
        <pressureVelocityCouplingScheme>SIMPLE</pressureVelocityCouplingScheme>
        <densityBasedSolverParameters>
            <formulation>implicit</formulation>
            <fluxType>roeFlux</fluxType>
            <entropyFixCoefficient>0.5</entropyFixCoefficient>
            <cutOffMachNumber>0.729</cutOffMachNumber>
        </densityBasedSolverParameters>
        <useMomentumPredictor>true</useMomentumPredictor>
        <discretizationSchemes>
            <time>firstOrderImplicit</time>
            <momentum>secondOrderUpwind</momentum>
            <energy>secondOrderUpwind</energy>
            <turbulentKineticEnergy>firstOrderUpwind</turbulentKineticEnergy>
            <volumeFraction>secondOrderUpwind</volumeFraction>
            <pressure>linear</pressure>
            <scalar>secondOrderUpwind</scalar>
            <species>secondOrderUpwind</species>
        </discretizationSchemes>
        <underRelaxationFactors>
            <pressure>0.3</pressure>
            <pressureFinal>1</pressureFinal>
            <density>0.9</density>
            <densityFinal>1</densityFinal>
            <momentum>0.7</momentum>
            <momentumFinal>1</momentumFinal>
            <energy>1.0</energy>
            <energyFinal>1</energyFinal>
            <turbulence>0.7</turbulence>
            <turbulenceFinal>1</turbulenceFinal>
            <volumeFraction>0.7</volumeFraction>
            <volumeFractionFinal>1</volumeFractionFinal>
            <scalar>0.7</scalar>
            <scalarFinal>1</scalarFinal>
            <species>1.0</species>
            <speciesFinal>1</speciesFinal>
        </underRelaxationFactors>
        <highOrderTermRelaxation disabled="true">
            <relaxationFactor>0.5</relaxationFactor>
        </highOrderTermRelaxation>
        <maxIterationsPerTimeStep>20</maxIterationsPerTimeStep>
        <numberOfCorrectors>4</numberOfCorrectors>
        <numberOfNonOrthogonalCorrectors>0</numberOfNonOrthogonalCorrectors>
        <multiphase>
            <maxIterationsPerTimeStep>2</maxIterationsPerTimeStep>
            <numberOfCorrectors>1</numberOfCorrectors>
            <useSemiImplicitMules>true</useSemiImplicitMules>
            <phaseInterfaceCompressionFactor>1</phaseInterfaceCompressionFactor>
            <numberOfMulesIterations>3</numberOfMulesIterations>
        </multiphase>
        <convergenceCriteria>
            <pressure>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </pressure>
            <density>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </density>
            <momentum>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </momentum>
            <energy>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </energy>
            <turbulence>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </turbulence>
            <volumeFraction>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </volumeFraction>
            <scalar>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </scalar>
            <species>
                <absolute>0.001</absolute>
                <relative>0.05</relative>
            </species>
        </convergenceCriteria>
        <advanced>
            <limits>
                <minimumStaticTemperature>1</minimumStaticTemperature>
                <maximumStaticTemperature>5000</maximumStaticTemperature>
                <maximumViscosityRatio>1e5</maximumViscosityRatio>
            </limits>
            <equations>
                <flow>true</flow>
                <energy disabled="false">
                    <includeViscousDissipationTerms>false</includeViscousDissipationTerms>
                    <includeKineticEnergyTerms>false</includeKineticEnergyTerms>
                    <includePressureWorkTerms>true</includePressureWorkTerms>
                </energy>
                <UDS>true</UDS>
                <species>true</species>
            </equations>
            <collateralFields>
                <age>false</age>
                <heatTransferCoefficient>false</heatTransferCoefficient>
                <machNumber>false</machNumber>
                <q>false</q>
                <totalPressure>false</totalPressure>
                <vorticity>false</vorticity>
                <wallHeatFlux>false</wallHeatFlux>
                <wallShearStress>false</wallShearStress>
                <wallYPlus>false</wallYPlus>
            </collateralFields>
        </advanced>
    </numericalConditions>

    <monitors>
        <forces/>
        <points/>
        <surfaces/>
        <volumes/>
    </monitors>

    <scaffolds>
        <boundaries/>
        <diskScaffolds/>
        <isoSurfaces/>
        <lineScaffolds/>
        <parallelograms/>
        <planeScaffolds/>
        <sphereScaffolds/>
    </scaffolds>

    <graphics>
    </graphics>

    <runCalculation>
        <runConditions>
            <numberOfIterations>1000</numberOfIterations>
            <timeSteppingMethod>fixed</timeSteppingMethod>
            <timeStepSize>0.01</timeStepSize>
            <maxCourantNumber>1</maxCourantNumber>
            <VoFMaxCourantNumber>1</VoFMaxCourantNumber>
            <maxDiffusionNumber>10</maxDiffusionNumber>
            <endTime>10</endTime>
            <reportIntervalSteps>100</reportIntervalSteps>
            <reportIntervalSeconds>0.1</reportIntervalSeconds>
            <retainOnlyTheMostRecentFiles>false</retainOnlyTheMostRecentFiles>
            <maximumNumberOfDataFiles>1</maximumNumberOfDataFiles>
            <dataWriteFormat>binary</dataWriteFormat>
            <dataWritePrecision>6</dataWritePrecision>
            <timePrecision>6</timePrecision>
        </runConditions>
        <parallel>
            <numberOfCores>1</numberOfCores>
            <localhost>true</localhost>
            <hostfile/>
        </parallel>
        <batch>
            <parameters/>
        </batch>
    </runCalculation>
</configuration>