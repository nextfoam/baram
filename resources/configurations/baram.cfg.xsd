<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" targetNamespace="http://www.baramcfd.org/baram"
           xmlns="http://www.baramcfd.org/baram" elementFormDefault="qualified" version="7">

    <!-- type definitions -->
    <xs:simpleType name="positive">
        <xs:restriction base="xs:double">
            <xs:minExclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="nonNegative">
        <xs:restriction base="xs:double">
            <xs:minInclusive value="0"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="ratio">
        <xs:restriction base="xs:double">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="exclusiveRatio">
        <xs:restriction base="xs:double">
            <xs:minExclusive value="0"/>
            <xs:maxExclusive value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="percent">
        <xs:restriction base="xs:double">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="100"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="degree180">
        <xs:restriction base="xs:double">
            <xs:minInclusive value="0"/>
            <xs:maxInclusive value="180"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="exclusvie0-10">
        <xs:restriction base="xs:double">
            <xs:minExclusive value="0"/>
            <xs:maxExclusive value="10"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:simpleType name="underRelaxationFactorNumber">
        <xs:restriction base="xs:double">
            <xs:minExclusive value="0"/>
            <xs:maxInclusive value="1"/>
        </xs:restriction>
    </xs:simpleType>

    <xs:complexType name="inputNumberType">
        <xs:simpleContent>
            <xs:extension base="xs:double">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputPositiveNumberType">
        <xs:simpleContent>
            <xs:extension base="positive">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputNonNegativeNumberType">
        <xs:simpleContent>
            <xs:extension base="nonNegative">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputRatioType">
        <xs:simpleContent>
            <xs:extension base="ratio">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputExclusiveRatioType">
        <xs:simpleContent>
            <xs:extension base="exclusiveRatio">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputPercentType">
        <xs:simpleContent>
            <xs:extension base="percent">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputDegree180Type">
        <xs:simpleContent>
            <xs:extension base="degree180">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="inputExclusive0-10Type">
        <xs:simpleContent>
            <xs:extension base="exclusvie0-10">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:complexType name="underRelaxationFactorNumberType">
        <xs:simpleContent>
            <xs:extension base="underRelaxationFactorNumber">
                <xs:attribute name="batchParameter" type="xs:string"/>
            </xs:extension>
        </xs:simpleContent>
    </xs:complexType>

    <xs:simpleType name="inputNumberListType">
        <xs:list itemType="xs:double"/>
    </xs:simpleType>

    <xs:complexType name="vector">
        <xs:sequence>
            <xs:element name="x" type="inputNumberType"/>
            <xs:element name="y" type="inputNumberType"/>
            <xs:element name="z" type="inputNumberType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="temporalScalarListType">
        <xs:sequence>
            <xs:element name="t" type="inputNumberListType"/>
            <xs:element name="v" type="inputNumberListType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="temporalVectorListType">
        <xs:sequence>
            <xs:element name="t" type="inputNumberListType"/>
            <xs:element name="x" type="inputNumberListType"/>
            <xs:element name="y" type="inputNumberListType"/>
            <xs:element name="z" type="inputNumberListType"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="scalarFieldType">
        <xs:sequence>
            <xs:element name="field" default="pressure">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="pressure"/>
                        <xs:enumeration value="speed"/>
                        <xs:enumeration value="xVelocity"/>
                        <xs:enumeration value="yVelocity"/>
                        <xs:enumeration value="zVelocity"/>
                        <xs:enumeration value="turbulentKineticEnergy"/>
                        <xs:enumeration value="turbulentDissipationRate"/>
                        <xs:enumeration value="specificDissipationRate"/>
                        <xs:enumeration value="modifiedTurbulentViscosity"/>
                        <xs:enumeration value="temperature"/>
                        <xs:enumeration value="density"/>
                        <xs:enumeration value="material"/>
                        <xs:enumeration value="scalar"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="fieldID" type="xs:positiveInteger" default="1"/>  <!-- set if "field" is "material" or "scalar" -->
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="userDefinedScalarListType">
        <xs:sequence>
            <xs:element name="scalar" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="scalarID" type="xs:positiveInteger"/>
                        <xs:element name="value" type="inputNumberType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="optionalUserDefinedScalarListType">
        <xs:sequence>
            <xs:element name="scalar" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="scalarID" type="xs:positiveInteger"/>
                        <xs:element name="value">
                            <xs:complexType>
                                <xs:simpleContent>
                                    <xs:extension base="inputNumberType">
                                        <xs:attribute name="disabled" type="xs:boolean" default="true"/>
                                    </xs:extension>
                                </xs:simpleContent>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="speciesListType">
        <xs:sequence>
            <xs:element name="mixture" minOccurs="0" maxOccurs="unbounded">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="mid" type="xs:positiveInteger"/>
                        <xs:element name="specie" minOccurs="1" maxOccurs="unbounded">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="mid" type="xs:positiveInteger"/>
                                    <xs:element name="value" type="inputNonNegativeNumberType"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="generalType">
        <xs:sequence>
            <xs:element name="flowType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="compressible"/>
                        <xs:enumeration value="incompressible"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="timeTransient" type="xs:boolean"/>
            <xs:element name="solverType">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="pressureBased"/>
                        <xs:enumeration value="densityBased"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="operatingConditions">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="gravity">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="direction" type="vector"/>
                                </xs:sequence>
                                <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="pressure" type="inputNumberType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="atmosphericBoundaryLayer">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="flowDirection" type="vector"/>
                        <xs:element name="groundNormalDirection" type="vector"/>
                        <xs:element name="referenceFlowSpeed" type="inputNumberType" default="10"/>
                        <xs:element name="referenceHeight" type="inputNumberType" default="10"/>
                        <xs:element name="surfaceRoughnessLength" type="inputNumberType" default="0.01"/>
                        <xs:element name="minimumZCoordinate" type="inputNumberType" default="0.0"/>
                        <xs:element name="userDefinedScalars" type="userDefinedScalarListType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="referenceValuesType">
        <xs:sequence>
            <xs:element name="area" type="inputNumberType" default="1"/>
            <xs:element name="density" type="inputNumberType" default="1.225"/>
            <xs:element name="length" type="inputNumberType" default="1"/>
            <xs:element name="velocity" type="inputNumberType" default="1"/>
            <xs:element name="pressure" type="inputNumberType" default="0"/>
            <xs:element name="referencePressureLocation" type="vector"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="materialType">
        <xs:all>
            <xs:element name="name" type="xs:string"/>
            <xs:element name="type">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="nonmixture"/>
                        <xs:enumeration value="mixture"/>
                        <xs:enumeration value="specie"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="chemicalFormula" type="xs:string" minOccurs="0"/>
            <xs:element name="phase">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="gas"/>
                        <xs:enumeration value="liquid"/>
                        <xs:enumeration value="solid"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="density">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="perfectGas"/>
                                    <xs:enumeration value="polynomial"/>
                                    <xs:enumeration value="incompressiblePerfectGas"/>
                                    <xs:enumeration value="PengRobinsonGas"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                        <xs:element name="pengRobinsonParameters" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="criticalTemperature" type="inputNumberType"/>
                                    <xs:element name="criticalPressure" type="inputNumberType"/>
                                    <xs:element name="criticalSpecificVolume" type="inputNumberType"/>
                                    <xs:element name="acentricFactor" type="inputNumberType"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="specificHeat">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="polynomial"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="viscosity" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="polynomial"/>
                                    <xs:enumeration value="sutherland"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputPositiveNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                        <xs:element name="sutherland" minOccurs="0">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="coefficient" type="inputNumberType"/>
                                    <xs:element name="temperature" type="inputNumberType"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="thermalConductivity">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="specification">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="constant"/>
                                    <xs:enumeration value="polynomial"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="constant" type="inputNumberType"/>
                        <xs:element name="polynomial" type="inputNumberListType"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="molecularWeight" type="inputNumberType" minOccurs="0"/>
            <xs:element name="absorptionCoefficient" type="inputNumberType" minOccurs="0"/>
            <xs:element name="saturationPressure" type="inputNumberType" minOccurs="0"/>
            <xs:element name="emissivity" type="inputNumberType" minOccurs="0"/>
            <xs:element name="mixture" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="massDiffusivity" type="inputPositiveNumberType"/>
                        <xs:element name="primarySpecie" type="xs:integer"/> <!-- 0 for not mixture type, or material mid-->
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="specie" minOccurs="0">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="mixture" type="xs:integer"/> <!-- 0 for not specie type, or material mid-->
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:all>
        <xs:attribute name="mid" type="xs:positiveInteger" use="required"/>
    </xs:complexType>

    <xs:complexType name="multiphaseModelsType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="off"/>
                        <xs:enumeration value="volumeOfFluid"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>
    <xs:complexType name="turbulenceModelsType">
        <xs:sequence>
            <xs:element name="model">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="inviscid"/>
                        <xs:enumeration value="laminar"/>
                        <xs:enumeration value="spalartAllmaras"/>
                        <xs:enumeration value="k-epsilon"/>
                        <xs:enumeration value="k-omega"/>
                        <xs:enumeration value="des"/>
                        <xs:enumeration value="les"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="k-epsilon">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="model">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="standard"/>
                                    <xs:enumeration value="rng"/>
                                    <xs:enumeration value="realizable"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="realizable">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="nearWallTreatment">
                                        <xs:simpleType>
                                            <xs:restriction base="xs:string">
                                                <xs:enumeration value="standardWallFunctions"/>
                                                <xs:enumeration value="enhancedWallTreatment"/>
                                            </xs:restriction>
                                        </xs:simpleType>
                                    </xs:element>
                                    <xs:element name="threshold" type="inputNumberType" default="60"/>
                                    <xs:element name="blendingWidth" type="inputNumberType" default="10"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="k-omega">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="model">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="SST"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="des">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="RANSModel">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="spalartAllmaras"/>
                                    <xs:enumeration value="kOmegaSST"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="spalartAllmarasOptions">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="lowReDamping" type="xs:boolean" default="false"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="DESOptions">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="delayedDES" type="xs:boolean" default="false"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="modelConstants">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="DES" type="inputNumberType" default="0.65"/>
                                    <xs:element name="DESKOmega" type="inputNumberType" default="0.82"/>
                                    <xs:element name="DESKEpsilon" type="inputNumberType" default="0.6"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                        <xs:element name="shieldingFunctions">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="DDES"/>
                                    <xs:enumeration value="IDDES"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="lengthScaleModel">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="cubeRootVol"/>
                                    <xs:enumeration value="vanDriest"/>
                                    <xs:enumeration value="smooth"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="les">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="subgridScaleModel">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="Smagorinsky"/>
                                    <xs:enumeration value="WALE"/>
                                    <xs:enumeration value="dynamicKEqn"/>
                                    <xs:enumeration value="kEqn"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="lengthScaleModel">
                            <xs:simpleType>
                                <xs:restriction base="xs:string">
                                    <xs:enumeration value="cubeRootVol"/>
                                    <xs:enumeration value="vanDriest"/>
                                    <xs:enumeration value="smooth"/>
                                </xs:restriction>
                            </xs:simpleType>
                        </xs:element>
                        <xs:element name="modelConstants">
                            <xs:complexType>
                                <xs:sequence>
                                    <xs:element name="k" type="inputNumberType" default="0.094"/>
                                    <xs:element name="e" type="inputNumberType" default="1.048"/>
                                    <xs:element name="w" type="inputNumberType" default="0.325"/>
                                </xs:sequence>
                            </xs:complexType>
                        </xs:element>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="energyPrandtlNumber" type="inputNumberType" default="0.85"/>
            <xs:element name="wallPrandtlNumber" type="inputNumberType" default="0.85"/>
            <xs:element name="turbulentSchmidtNumber" type="inputPositiveNumberType" default="0.7"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="mrfZoneType">
        <xs:sequence>
            <xs:element name="rotatingSpeed" type="inputNumberType" default="0"/>
            <xs:element name="rotationAxisOrigin" type="vector"/>
            <xs:element name="rotationAxisDirection" type="vector"/>
            <xs:element name="staticBoundaries">
                <xs:simpleType>
                    <xs:list itemType="xs:positiveInteger"/>  <!-- boundary condition bcid -->
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="porousZoneType">
        <xs:sequence>
            <xs:element name="model" default="darcyForchheimer">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="darcyForchheimer"/>
                        <xs:enumeration value="powerLaw"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
            <xs:element name="darcyForchheimer">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="direction1Vector" type="vector"/>
                        <xs:element name="direction2Vector" type="vector"/>
                        <xs:element name="viscousResistanceCoefficient" type="vector"/>
                        <xs:element name="inertialResistanceCoefficient" type="vector"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
            <xs:element name="powerLaw">
                <xs:complexType>
                    <xs:sequence>
                        <xs:element name="c0" type="inputNumberType" default="0"/>
                        <xs:element name="c1" type="inputNumberType" default="0"/>
                    </xs:sequence>
                </xs:complexType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="slidingMeshZoneType">
        <xs:sequence>
            <xs:element name="rotatingSpeed" type="inputNumberType" default="1000"/>
            <xs:element name="rotationAxisOrigin" type="vector"/>
            <xs:element name="rotationAxisDirection" type="vector"/>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="actuatorDiskZoneType">
        <xs:sequence>
            <xs:element name="diskDirection" type="vector"/>
            <xs:element name="powerCoefficient" type="inputNumberType" default="0"/>
            <xs:element name="thrustCoefficient" type="inputNumberType" default="0"/>
            <xs:element name="diskArea" type="inputNumberType" default="0"/>
            <xs:element name="upstreamPoint" type="vector"/>
            <xs:element name="forceComputation" default="Froude">
                <xs:simpleType>
                    <xs:restriction base="xs:string">
                        <xs:enumeration value="Froude"/>
                        <xs:enumeration value="variableScaling"/>
                    </xs:restriction>
                </xs:simpleType>
            </xs:element>
        </xs:sequence>
    </xs:complexType>

    <xs:complexType name="volumeFractionType">
        <xs:sequence>
            <xs:element name="material" type="xs:positiveInteger"/> <!-- material mid -->
            <xs:element name="fraction" type="inputRatioType" default="0"/>
        </xs:sequence>
    </xs:complexType>

    <xs:element name="cellZone">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string"/>
                <xs:element name="zoneType" default="none">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="none"/>
                            <xs:enumeration value="mrf"/>
                            <xs:enumeration value="porous"/>
                            <xs:enumeration value="actuatorDisk"/>
                            <xs:enumeration value="slidingMesh"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
