                <xs:element name="showChart" type="xs:boolean" default="true"/>
                <xs:element name="writeInterval" type="inputNumberType"/>
                <xs:element name="reportType" default="areaWeightedAverage">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="areaWeightedAverage"/>
                            <xs:enumeration value="massWeightedAverage"/>
                            <xs:enumeration value="Integral"/>
                            <xs:enumeration value="massFlowRate"/>
                            <xs:enumeration value="volumeFlowRate"/>
                            <xs:enumeration value="minimum"/>
                            <xs:enumeration value="maximum"/>
                            <xs:enumeration value="cov"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="field" type="scalarFieldType"/>
                <xs:element name="surface" type="xs:nonNegativeInteger"/>  <!-- boundary condition bcid -->
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <xs:element name="volumeMonitor">
        <xs:complexType>
            <xs:sequence>
                <xs:element name="name" type="xs:string" default="volumeAverage"/>
                <xs:element name="showChart" type="xs:boolean" default="true"/>
                <xs:element name="writeInterval" type="inputNumberType"/>
                <xs:element name="reportType">
                    <xs:simpleType>
                        <xs:restriction base="xs:string">
                            <xs:enumeration value="volumeAverage"/>
                            <xs:enumeration value="volumeIntegral"/>
                            <xs:enumeration value="minimum"/>
                            <xs:enumeration value="maximum"/>
                            <xs:enumeration value="cov"/>
                        </xs:restriction>
                    </xs:simpleType>
                </xs:element>
                <xs:element name="field" type="scalarFieldType"/>
                <xs:element name="volume" type="xs:nonNegativeInteger"/>   <!-- cellZone czid -->
            </xs:sequence>
        </xs:complexType>
    </xs:element>

    <!-- root element "configuration" -->

    <xs:element name="configuration">
        <xs:complexType>
            <xs:sequence>

                <xs:element name="general" type="generalType"/>

                <xs:element name="referenceValues" type="referenceValuesType"/>

                <xs:element name="materials">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="material" type="materialType" minOccurs="0" maxOccurs="unbounded"/>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="models">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="multiphaseModels" type="multiphaseModelsType"/>
                            <xs:element name="energyModels">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="off"/>
                                        <xs:enumeration value="on"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="turbulenceModels" type="turbulenceModelsType"/>
                            <xs:element name="speciesModels">
                                <xs:simpleType>
                                    <xs:restriction base="xs:string">
                                        <xs:enumeration value="off"/>
                                        <xs:enumeration value="on"/>
                                    </xs:restriction>
                                </xs:simpleType>
                            </xs:element>
                            <xs:element name="userDefinedScalars">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="scalar" minOccurs="0" maxOccurs="unbounded">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="fieldName" type="xs:string"/>
                                                    <xs:element name="region" type="xs:string"/>
                                                    <xs:element name="material" type="xs:integer"/> <!-- 0 for 'All', or material mid-->
                                                    <xs:element name="diffusivity">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="specificationMethod" default="constant">
                                                                    <xs:simpleType>
                                                                        <xs:restriction base="xs:string">
                                                                            <xs:enumeration value="constant"/>
                                                                            <xs:enumeration value="turbulentViscosity"/>
                                                                            <xs:enumeration value="laminarAndTurbulentViscosity"/>
                                                                        </xs:restriction>
                                                                    </xs:simpleType>
                                                                </xs:element>
                                                                <xs:element name="constant" type="inputNumberType"/>
                                                                <xs:element name="laminarAndTurbulentViscosity">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="laminarViscosityCoefficient" type="inputNumberType" default="0"/>
                                                                            <xs:element name="turbulentViscosityCoefficient" type="inputNumberType" default="0"/>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                                <xs:attribute name="scalarID" type="xs:integer" use="required"/>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="regions">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="region" minOccurs="0" maxOccurs="unbounded">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="name" type="xs:string"/>
                                        <xs:element name="material" type="xs:positiveInteger"/> <!-- material mid -->
                                        <xs:element name="secondaryMaterials">
                                            <xs:simpleType>
                                                <xs:list itemType="xs:positiveInteger"/>  <!-- secondary material mid -->
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="phaseInteractions">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="surfaceTensions">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="surfaceTension" minOccurs="0" maxOccurs="unbounded">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="mid" type="xs:positiveInteger" minOccurs="2" maxOccurs="2"/><!-- material mid -->
                                                                            <xs:element name="value" type="inputNumberType"/>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="cellZones">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element ref="cellZone" minOccurs="0" maxOccurs="unbounded"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="boundaryConditions">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element ref="boundaryCondition" minOccurs="0" maxOccurs="unbounded"/>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                        <xs:element name="initialization">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="initialValues">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="velocity" type="vector"/>
                                                                <xs:element name="pressure" type="inputNumberType" default="0"/>
                                                                <xs:element name="temperature" type="inputNumberType" default="300"/>
                                                                <xs:element name="scaleOfVelocity" type="inputNumberType" default="1"/>
                                                                <xs:element name="turbulentIntensity" type="inputPercentType" default="1"/>
                                                                <xs:element name="turbulentViscosity" type="inputNumberType" default="10"/>
                                                                <xs:element name="volumeFractions" type="volumeFractionListType"/>
                                                                <xs:element name="userDefinedScalars" type="optionalUserDefinedScalarListType"/>
                                                                <xs:element name="species" type="speciesListType"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                    <xs:element name="advanced">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="sections">
                                                                    <xs:complexType>
                                                                        <xs:sequence>
                                                                            <xs:element name="section" minOccurs="0" maxOccurs="unbounded">
                                                                                <xs:complexType>
                                                                                    <xs:sequence>
                                                                                        <xs:element name="name" type="xs:string"/>
                                                                                        <xs:element name="type">
                                                                                            <xs:simpleType>
                                                                                                <xs:restriction base="xs:string">
                                                                                                    <xs:enumeration value="hex"/>
                                                                                                    <xs:enumeration value="cylinder"/>
                                                                                                    <xs:enumeration value="sphere"/>
                                                                                                    <xs:enumeration value="cellZone"/>
                                                                                                </xs:restriction>
                                                                                            </xs:simpleType>
                                                                                        </xs:element>
                                                                                        <xs:element name="point1" type="vector"/> <!-- Used as hexMin, axisPoint1, centerPoint -->
                                                                                        <xs:element name="point2" type="vector"/> <!-- Used as hexMax, axisPoint2 -->
                                                                                        <xs:element name="radius" type="inputNumberType" default="0"/>
                                                                                        <xs:element name="cellZone" type="xs:positiveInteger"/>   <!-- cellZone czid -->
                                                                                        <xs:element name="velocity">
                                                                                            <xs:complexType>
                                                                                                <xs:complexContent>
                                                                                                    <xs:extension base="vector">
                                                                                                        <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                                                                                                    </xs:extension>
                                                                                                </xs:complexContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                        <xs:element name="pressure">
                                                                                            <xs:complexType>
                                                                                                <xs:simpleContent>
                                                                                                    <xs:extension base="inputNumberType">
                                                                                                        <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                                                                                                    </xs:extension>
                                                                                                </xs:simpleContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                        <xs:element name="temperature">
                                                                                            <xs:complexType>
                                                                                                <xs:simpleContent>
                                                                                                    <xs:extension base="inputNumberType">
                                                                                                        <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                                                                                                    </xs:extension>
                                                                                                </xs:simpleContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                        <xs:element name="volumeFractions">
                                                                                            <xs:complexType>
                                                                                                <xs:complexContent>
                                                                                                    <xs:extension base="volumeFractionListType">
                                                                                                        <xs:attribute name="disabled" type="xs:boolean" use="required"/>
                                                                                                    </xs:extension>
                                                                                                </xs:complexContent>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                        <xs:element name="userDefinedScalars" type="optionalUserDefinedScalarListType"/>
                                                                                        <xs:element name="species">
                                                                                            <xs:complexType>
                                                                                                <xs:sequence>
                                                                                                    <xs:element name="mixture" minOccurs="0" maxOccurs="unbounded">
                                                                                                        <xs:complexType>
                                                                                                            <xs:sequence>
                                                                                                                <xs:element name="mid" type="xs:positiveInteger"/>
                                                                                                                <xs:element name="specie" minOccurs="1" maxOccurs="unbounded">
                                                                                                                    <xs:complexType>
                                                                                                                        <xs:sequence>
                                                                                                                            <xs:element name="mid" type="xs:positiveInteger"/>
                                                                                                                            <xs:element name="value" type="inputNonNegativeNumberType"/>
                                                                                                                        </xs:sequence>
                                                                                                                    </xs:complexType>
                                                                                                                </xs:element>
                                                                                                            </xs:sequence>
                                                                                                            <xs:attribute name="disabled" type="xs:boolean" default="true"/>
                                                                                                        </xs:complexType>
                                                                                                    </xs:element>
                                                                                                </xs:sequence>
                                                                                            </xs:complexType>
                                                                                        </xs:element>
                                                                                        <xs:element name="overrideBoundaryValue" type="xs:boolean" default="false"/>
                                                                                    </xs:sequence>
                                                                                </xs:complexType>
                                                                            </xs:element>
                                                                        </xs:sequence>
                                                                    </xs:complexType>
                                                                </xs:element>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="numericalConditions" type="numericalConditionsType"/>

                <xs:element name="monitors">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="forces">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="forceMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="points">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="pointMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="surfaces">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="surfaceMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="volumes">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element ref="volumeMonitor" minOccurs="0"
                                                    maxOccurs="unbounded"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>

                <xs:element name="runCalculation">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="runConditions">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="numberOfIterations" type="xs:nonNegativeInteger" default="0"/>
                                        <xs:element name="timeSteppingMethod" default="fixed">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="fixed"/>
                                                    <xs:enumeration value="adaptive"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="timeStepSize" type="inputNumberType" default="0.01"/>
                                        <xs:element name="maxCourantNumber" type="inputNumberType" default="1"/>
                                        <xs:element name="VoFMaxCourantNumber" type="inputNumberType" default="1"/>
                                        <xs:element name="endTime" type="inputNumberType" default="0"/>
                                        <xs:element name="reportIntervalSteps" type="xs:positiveInteger" default="1"/>
                                        <xs:element name="reportIntervalSeconds" type="inputNumberType" default="0.01"/>
                                        <xs:element name="retainOnlyTheMostRecentFiles" type="xs:boolean"
                                                    default="false"/>
                                        <xs:element name="maximumNumberOfDataFiles" type="xs:positiveInteger"
                                                    default="1"/>
                                        <xs:element name="dataWriteFormat" default="binary">
                                            <xs:simpleType>
                                                <xs:restriction base="xs:string">
                                                    <xs:enumeration value="binary"/>
                                                    <xs:enumeration value="ascii"/>
                                                </xs:restriction>
                                            </xs:simpleType>
                                        </xs:element>
                                        <xs:element name="dataWritePrecision" type="xs:positiveInteger" default="6"/>
                                        <xs:element name="timePrecision" type="xs:positiveInteger" default="6"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="parallel">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="numberOfCores" type="xs:positiveInteger"/>
                                        <xs:element name="localhost" type="xs:boolean"/>
                                        <xs:element name="hostfile" type="xs:string"/>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                            <xs:element name="batch">
                                <xs:complexType>
                                    <xs:sequence>
                                        <xs:element name="parameters">
                                            <xs:complexType>
                                                <xs:sequence>
                                                    <xs:element name="parameter" minOccurs="0" maxOccurs="unbounded">
                                                        <xs:complexType>
                                                            <xs:sequence>
                                                                <xs:element name="name" type="xs:string"/>
                                                                <xs:element name="value" type="xs:string"/>
                                                            </xs:sequence>
                                                        </xs:complexType>
                                                    </xs:element>
                                                </xs:sequence>
                                            </xs:complexType>
                                        </xs:element>
                                    </xs:sequence>
                                </xs:complexType>
                            </xs:element>
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:sequence>
            <xs:attribute name="version" type="xs:positiveInteger" use="required"/>
        </xs:complexType>
    </xs:element>
</xs:schema>
